  const todoCollection = collection(db,\"todos\");\n\n    const { addToast } = useToasts();\n\n    // const [snapshot, loading, error] = useCollection(collection, {\n    //     snapshotListenOptions: { includeMetadataChanges: true },\n    // });\n\n    const addTodo = useCallback(async(formData)=>{\n        const newTodo = { ...formData };\n        console.log(\"new todo\", newTodo);\n        setTodos([...todos, newTodo]);\n\n        try{\n            const docRef = await addDoc(todoCollection,formData);\n            console.log('added todo id: ', docRef.id);\n            addToast(`Added ${newTodo.todoName}`, {\n                appearance: \"success\",\n            });\n        } catch(err) {\n            console.log(err)\n            addToast(`Error Adding ${newTodo.todoName}`, {\n                appearance: \"error\",\n            });\n        }\n        \n    },[todos, addToast, todoCollection]);\n\n\n    const getTodos = useCallback(async()=>{\n        setLoading(true);\n        const todoQuery = query(\n            todoCollection,\n            orderBy('done'),\n            limit(20)\n        );\n        console.log('todoQuery: ', todoQuery);\n        const currentTodos = [];\n        try{\n            const querySnapshot = await getDocs(todoQuery);\n            console.log('querySnapshot: ', querySnapshot);\n            // onSnapshot(todoQuery, (querySnapshot)=>{\n                querySnapshot.forEach((todo)=>{\n                    // console.log(`Todo: ${todo.id}: ${JSON.stringify(todo.data())}`)\n                    const data = {...todo.data(), 'id': todo.id}\n                    console.log(data);\n                    currentTodos.push(data);\n                    // setTodos([{...todo.data(), id: todo.id}])\n                })\n            // })\n            console.log('current todos: ', currentTodos);\n            setTodos(currentTodos);\n            console.log('todos after get and set: ', todos); \n        } catch(err) {\n            console.log(err);\n        } finally {\n            setLoading(false);\n            setLoaded(true);\n        }\n    },[todoCollection, todos, setTodos]);\n\n    const deleteTodo = useCallback(async(id)=>{\n        const docRef = doc(todoCollection, id);\n        try {\n            const docSnapshot = await getDoc(docRef);\n            if(docSnapshot.exists()){\n                console.log(\"Document data:\", docSnapshot.data());\n                await deleteDoc(doc(todoCollection, docSnapshot.id));\n                addToast(`Deleted ${docSnapshot.data().todoName}`, {\n                    appearance: \"success\",\n                });\n            }\n        } catch(err) {\n            console.log(err)\n            addToast(`Error Deleting Todo`, {\n                appearance: \"error\",\n            });\n        }\n    },[addToast, todoCollection]);\n\n    const updateTodo = useCallback(async(id)=>{\n        const docRef = doc(todoCollection, id);\n        try{\n            const docSnapshot = await getDoc(docRef);\n            console.log(\"Document data:\", docSnapshot.data());\n            docSnapshot.data().done?setDoc(docRef, {done:false}, {merge:true}):setDoc(docRef, {done:true}, {merge:true});\n        } catch(err) {\n            console.log(err)\n        }\n    },[todoCollection])\n\n    return (\n        <TodoContext.Provider\n            value={{\n            todos,\n            addTodo,\n            getTodos,\n            deleteTodo,\n            updateTodo,\n            loading,\n            loaded\n        }}\n        >\n            {props.children}\n        </TodoContext.Provider>\n    );\n};","/Users/ayibacesario/Documents/bootcamp/to-do-firebase/src/contexts/firebase.js",[],[],"/Users/ayibacesario/Documents/bootcamp/to-do-firebase/src/components/TodoList/TodoList.js",["77","78","79","80","81","82","83","84","85","86","87"],[],"/Users/ayibacesario/Documents/bootcamp/to-do-firebase/src/components/ErrorBoundary/ErrorBoundary.js",[],[],{"ruleId":"88","severity":1,"message"